{
  "name": "@muditor/db",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "prisma generate --generator client",
    "push": "prisma db push --skip-generate && pnpm generate",
    "studio": "prisma studio",
    "migrate:dev": "prisma migrate dev --skip-generate && pnpm generate",
    "migrate:reset": "prisma migrate reset --skip-generate && pnpm generate",
    "migrate:deploy": "prisma migrate deploy --skip-generate && pnpm generate",
    "seed": "tsx src/seed/index.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules/.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "bcrypt": "^6.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/client-preset": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.9.2",
    "dotenv": "^17.2.2",
    "husky": "^9.1.7",
    "prisma": "^6.16.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
