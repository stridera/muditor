enum RoomFlag {
  DARK
  DEATH
  NOMOB
  INDOORS
  PEACEFUL
  SOUNDPROOF
  NOTRACK
  NOMAGIC
  TUNNEL
  PRIVATE
  GODROOM
  HOUSE
  HOUSECRASH
  ATRIUM
  OLC
  BFS_MARK
  WORLDMAP
  FERRY_DEST
  ISOLATED
  ARENA
  LARGE
  MEDIUM_LARGE
  MEDIUM
  MEDIUM_SMALL
  SMALL
  VERY_SMALL
  ONE_PERSON
  EFFECTS_NEXT
}

enum TriggerFlag {
  Global
  Random
  Command
  Speech
  Act
